{
  "name": "avantia-back",
  "version": "1.0.0",
  "description": "An API for a mask detection AI",
  "main": "dist/server.js",
  "repository": "https://github.com/CITi-UFPE/avantia-back",
  "author": "Edgar Marques <edgarmarques.110@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node -r source-map-support/register .",
    "compile-ts": "tsc",
    "compile-static": "cp ./src/frontend/index.html ./dist/frontend/index.html && cp ./src/frontend/static/main.css ./dist/frontend/static/main.css",
    "watch-ts": "nodemon -e ts --exec \"yarn compile-ts\"",
    "watch-static": "nodemon -e html,css --exec \"yarn compile-static\"",
    "postinstall": "npm run compile-ts && npm run compile-static"
  },
  "engines": {
    "node": "13.8.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-rtsp-stream": "^0.0.9",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/form-data": "^2.5.0",
    "@types/helmet": "^0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.25",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "concurrently": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "source-map-support": "^0.5.19"
  }
}
